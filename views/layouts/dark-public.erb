/* 1) Wrap both in a fixed-height container */
.code-container {
  position: relative;
  width: 100%;
  height: 250px;           /* or whatever height you need */
  overflow: hidden;        /* so scrollbar is on both at once */
}

/* 2) Make pre & textarea exactly overlap */
.code-container pre,
.code-container textarea {
  position: absolute;
  top:    0;
  left:   0;
  right:  0;
  bottom: 0;
  margin: 0;
  padding: 12px 16px;      /* copy your .form-control padding exactly */
  box-sizing: border-box;
  width:  100%;
  height: 100%;
  font-family: Menlo, Consolas, "Courier New", monospace;
  font-size:   1rem;       /* match your textarea’s font-size */
  line-height: 1.5;        /* match your textarea’s line-height */
  white-space: pre-wrap;   /* preserve line-breaks & spaces */
  word-wrap: break-word;   /* wrap long lines */
  overflow: auto;          /* enable scroll when needed */
}

/* 3) The highlighter is behind */
.code-container pre {
  z-index: 0;
  color: #ddd;             /* your default text color */
  pointer-events: none;    /* clicks go through */
}

/* 4) The textarea is on top, but transparent */
.code-container textarea {
  z-index:     1;
  background:  transparent !important;
  color:       transparent !important;
  caret-color: #fff;       /* keep the caret visible */
  resize:      none;       /* optional: lock resize to container */
  border:      none;       /* assume container handles border */
}

