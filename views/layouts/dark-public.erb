// update user preference via AJAX
function setPreference(name, deflt, value, successCallBack = undefined, errorCallBack = undefined) {
	// Prepare the data to be sent in the request
	const data = {
		name: name,       		// Preference name
		default: deflt, 		// Default value
		value: value        	// New value ('yes' or 'no')
	};

	// Make the AJAX POST request to update the preference
	fetch('/ajax/set_preference.json', {
		method: 'POST',                      	// HTTP method
		headers: {
			'Content-Type': 'application/json' 	// Specify JSON content
		},
		body: JSON.stringify(data)           	// Convert data to JSON string
	})
	.then(response => {
		if (!response.ok) {
			// Handle HTTP errors
			throw new Error(`Network response was not ok: ${response.statusText}`);
		}
		return response.json(); // Parse the JSON response
	})
	.then(data => {
		if (data.status !== 'success') {
			// Handle application-level errors
			if(errorCallBack && typeof errorCallBack  === 'function') {
				errorCallBack(data);
			}
			console.error('Error updating preference:', data.status);
		} else {
			//console.log('Preference updated successfully.');
			if(successCallBack && typeof successCallBack  === 'function') {
				successCallBack(data);
			}
		}
	})
	.catch(error => {
		// Handle fetch or parsing errors
		if(errorCallBack && typeof errorCallBack  === 'function') {
			errorCallBack(data);
		}
		console.error('There was a problem with the fetch operation:', error);
	});
}
	
