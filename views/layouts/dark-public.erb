			}
			
			span.bright-underline::after {
				content: "";
				position: absolute;
				left: 0;
				bottom: 0; /* adjust as needed */
				width: 100%;
				height: 2px; /* thickness of the "underline" */
				background-color: yellow; //rgb(255,90,90); /* the underline color */
				box-shadow: 0 0 3px white, 0 0 6px white; /* bright white shadow/glow */
			}
		
			span.old-price {
				position: relative;
				color: white;
				font-weight: bolder;
				text-decoration: none; /* Remove default strike-through */
			}
			
			span.old-price::before {
				content: "";
				position: absolute;
				left: 0;
				right: 0;
				top: 50%; /* Position the line in the middle of the text */
				height: 3px; /* Thickness of the strike-through: 3 pixels */
				background-color: red; /* Red line */
				box-shadow: 0 0 3px white, 0 0 6px white; /* White bright shadow */
				transform: translateY(-50%); /* Center the line vertically */
			}
		

