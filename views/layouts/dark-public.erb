			/* ——————————————————————————————
			SCOPE EVERYTHING TO “*-filter” CONTAINERS
			—————————————————————————————— */
			[id$="-filter"] {
			/* full width */
			width: 100%;
			}

			/* 1) label above the textbox */
			[id$="-filter"] .control-label {
			display: block;
			margin-bottom: 0.25rem;
			color: #fff;
			font-weight: 600;
			}

			/* 2) make the input+buttons row flex */
			[id$="-filter"] .row-fluid {
			display: flex;
			align-items: center;
			flex-wrap: wrap;
			margin-bottom: 0.75rem; /* space before the tags list */
			}

			/* 3) style the text‑input like your .dark-input */
			[id$="-filter"] input[type="text"] {
			background-color: #000;
			border: 2px solid #7f56d9;
			border-radius: 4px;
			color: #7f56d9;
			font-weight: bold;
