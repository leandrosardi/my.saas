			/* General Testimonial Section */
			.about-testimonial {
				padding: 60px 20px;
				background-color: #121212; /* Dark background */
				text-align: center;
			}
			
			/* Testimonials Grid Layout */
			.testimonial-container {
				display: grid;
				grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); /* Responsive grid */
				gap: 20px; /* Space between cards */
				justify-content: center;
				align-items: start;
				max-width: 1200px;
				margin: auto;
			}
			
			/* Testimonial Card */
			.testimonial-card {
				background: #1a1a1a; /* Dark card background */
				padding: 20px;
				border-radius: 12px;
				box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.3);
				text-align: left;
				color: #ffffff;
				transition: transform 0.3s ease-in-out;
			}
			
			.testimonial-card:hover {
				transform: translateY(-5px); /* Lift effect on hover */
			}
			
			/* Profile Image */
			.testimonial-card img {
				width: 60px;
				height: 60px;
				border-radius: 50%;
				object-fit: cover;
				margin-bottom: 10px;
			}
			
			/* Name and Position */
			.testimonial-name {
				font-size: 18px;
				font-weight: bold;
				color: #ffffff;
			}
			
			.testimonial-role {
				font-size: 14px;
				color: #bbb;
				margin-bottom: 10px;
			}
			
			/* Company Link */
			.testimonial-company a {
				color: #4ca1ff;
				text-decoration: none;
				font-weight: bold;
			}
			
			.testimonial-company a:hover {
				text-decoration: underline;
			}
			
			/* Testimonial Quote */
			.testimonial-text {
				font-size: 14px;
				color: #ddd;
				margin-top: 10px;
			}
		






